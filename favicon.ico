<!-- Spotify-like icon (for illustrative / personal use) -->
<svg xmlns="http://www.w3.org/2000/svg"
     viewBox="0 0 100 100"
     width="100" height="100"
     role="img" aria-labelledby="title-desc">
  <title id="title-desc">Spotify icon</title>

  <!-- Background circle (Spotify green) -->
  <circle cx="50" cy="50" r="48" fill="#1DB954"/>

  <!-- Three rounded "sound" arcs -->
  <!-- Adjust stroke-width to make arcs thicker/thinner -->
  <g stroke="#FFFFFF" stroke-width="6.8" stroke-linecap="round" fill="none">
    <!-- top arc -->
    <path d="M26 35 C42 30, 58 30, 74 35" />
    <!-- middle arc -->
    <path d="M24 50 C42 44, 58 44, 76 50" />
    <!-- bottom arc -->
    <path d="M28 65 C44 59, 56 59, 72 65" />
  </g>
</svg>
